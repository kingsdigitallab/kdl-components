{% extends "../../component.njk" %}

{% block component %}
<div class="kdl-header:title">
  {% if kwargs.url %}
  <a class="kdl-header:link" href="{{ kwargs.url }}">{{ kwargs.title }}</a>
  {% else %}
  {{- kwargs.title -}}
  {% endif %}
</div>
{% if kwargs.logo %}
<div class="kdl-header:logo">
  <img src="{{ kwargs.logo.src }}" alt="{{ kwargs.logo.alt }}">
</div>
{% endif %}
{% endblock %}
