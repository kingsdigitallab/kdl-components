{% extends "../../component.njk" %}

{% block component %}
  <div>
    <a href="{{ data.url | default('/') }}">{{ data.title }}</a>
  </div>
  {% if data.feature %}
    <div>
      <img src="{{ data.feature.image }}" alt="{{ data.feature.description }}">
    </div>
  {% endif %}
{% endblock component %}
